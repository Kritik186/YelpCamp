

<div class="container">
    <div class="jumbotron">
        <h1>This the campgrounds page</h1>
        <h2>The campgrounds are as follows</h2>
        <a class="btn btn-primary btn-large" href="/campgrounds/new"> Add a new Campground</a>
    </div>
   
    <div class="row container">
        <%campgrounds.forEach(function(camps){%>
            <div class="col-md-4 col-sm-6">
                <div class="thumbnail">
                    <img src=" <%=camps.image%> " >
                    <div class="caption"><h4><%=camps.title%></h4></div>
                    <a class="btn btn-primary" href="/campgrounds/<%=camps._id%>">Description</a>
                </div>
             </div>   
        <%})%>
    </div>
    
</div>


